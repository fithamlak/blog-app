<main class='content container'>
    <h1>List of posts of <%= @user&.name %></h1>
    <ul>
    <li><%= render @user %></li>
    <% @posts.each do |post| %>
      <li><%= render post %></li>
      <% post.last_five_comments.each do |comment| %>
        <li><%= render 'comment', comment: comment %></li>
      <% end %>
    <% end %>
    </ul>
    <div class="text-center">
      <button class='btn btn-primary'>Pagination</button>
    </div>
  </main>